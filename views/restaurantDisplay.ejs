<!DOCTYPE html>
<html>
<head>
    <title>Restaurant Display</title>
    <!-- <link rel="stylesheet" type="text/css" href="/path/to/your/css/file.css"> -->
</head>
<body>
    <h1>Restaurant Display</h1>
    <ul>
        <% Restaurants.forEach(function(Restaurants) { %>
            <li>
                <h2><%= Restaurants.name %></h2>
                <p><%= Restaurants.description %></p>
                <p>Address: <%= Restaurants.address %></p>
                <img src="data:image/jpeg;base64,${Buffer.from(Restaurants.Photo).toString('base64')}" alt="Restaurant Photo">
            </li>
        <% }); %>
    </ul>
</body>
</html>
