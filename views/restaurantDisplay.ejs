<!DOCTYPE html>
<html>
<head>
    <title>Restaurant Display</title>
    <!-- <link rel="stylesheet" type="text/css" href="/path/to/your/css/file.css"> -->
</head>
<body>
    <h1>Restaurant Display</h1>
    <ul>
        <% Restaurants.forEach(function(Restaurants) { %>
            <li>
                <h2><%= Restaurants.Restaurant_Name %></h2>
                <p>Address: <%= Restaurants.Address %></p>
                <p>Photo: <%= Restaurants.Photo %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>
